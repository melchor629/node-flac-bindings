{
  "name": "flac-bindings",
  "version": "2.7.0",
  "libflac": "1.3.3",
  "libogg": "1.3.4",
  "description": "libflac bindings to node.js with easy to use API",
  "repository": "https://github.com/melchor629/node-flac-bindings/",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "coverage": "./scripts/coverage.sh",
    "install": "node scripts/flac-build.js",
    "prebuild": "prebuild --backend cmake-js --runtime napi -t 8 --strip",
    "lint": "eslint --ext js lib test examples"
  },
  "keywords": [
    "flac",
    "encoder",
    "decoder",
    "binding"
  ],
  "author": "melchor9000",
  "license": "ISC",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.2.1",
    "debug": "^4.3.2",
    "node-addon-api": "^4.1.0",
    "prebuild-install": "^6.1.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.14",
    "@types/temp": "^0.9.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.1.0",
    "jest-extended": "^0.11.5",
    "prebuild": "^10.0.1",
    "temp": "^0.9.4"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "test/",
      "node_modules"
    ],
    "coverageProvider": "v8",
    "coverageReporters": [
      [
        "lcovonly",
        {
          "file": "js.info"
        }
      ],
      "text"
    ],
    "roots": [
      "test"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "node",
    "transform": {}
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/melchor629"
  }
}
