{
  "name": "flac-bindings",
  "version": "2.2.0",
  "description": "libflac bindings to node.js with easy to use API",
  "repository": "https://github.com/melchor629/node-flac-bindings/",
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "mocha --recursive",
    "install": "prebuild-install || cmake-js compile",
    "cmake-js": "cmake-js",
    "prebuild": "prebuild --backend cmake-js --runtime napi --strip",
    "lint": "eslint -c .eslintrc.json lib/**/*.js test/**/*.js"
  },
  "keywords": [
    "flac",
    "encoder",
    "decoder",
    "binding"
  ],
  "author": "melchor9000",
  "license": "ISC",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.0.0",
    "node-addon-api": "git+https://github.com/nodejs/node-addon-api.git",
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.22",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.2.2",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "prebuild": "^10.0.0",
    "temp": "^0.9.1"
  },
  "binary": {
    "napi_versions": [
      4
    ]
  }
